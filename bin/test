#! /bin/sh

curl -H "Content-Type: application/json" -X POST -d '{ "quote": { "text": { "en": "Work is not a wolf, it will not run away"}, "author": { "en": "Russian proverb" }, "status": "completed" } }' http://0.0.0.0:8080/quotes/create_quote

curl -H "Content-Type: application/json" -X POST -d '' http://0.0.0.0:8080/quotes/get_random_quote